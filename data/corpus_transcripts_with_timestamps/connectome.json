            ]
         },
         {
            "Reciever":"4",
            "Sequence":"5",
            "Sentence":[
               [
                  "00:03:22,900 --> 00:03:25,420",
                  "Awesome, good."
               ],
               [
                  "00:03:25,420 --> 00:03:30,190",
