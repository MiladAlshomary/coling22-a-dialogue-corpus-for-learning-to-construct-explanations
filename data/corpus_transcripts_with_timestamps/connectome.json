            ]
         },
         {
            "Reciever":"4",
            "Sequence":"7",
            "Sentence":[
               [
                  "00:03:44,590 --> 00:03:46,960",
                  "Yes."
               ]
            ]
         },
         {
            "Reciever":"4",
            "Sequence":"9",
            "Sentence":[
               [
                  "00:03:52,900 --> 00:03:54,880",
